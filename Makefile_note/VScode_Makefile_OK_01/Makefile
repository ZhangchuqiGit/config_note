
CCFLAGS = ${CFLAG}
# CCFLAGS += -Wall -std=c++17 
CCFLAGS += -Wall
CCFLAGS += -std=c++17 

all:main
		
main:
	g++ ${CCFLAGS} main.cpp -o all

.PHONY: clean # .PHONY 表示 clean 是个伪目标
clean :
	rm -rf all


